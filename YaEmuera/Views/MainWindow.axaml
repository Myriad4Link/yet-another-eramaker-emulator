<Window xmlns="https://github.com/avaloniaui"        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"        xmlns:app="using:YaEmuera"        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"        x:Class="YaEmuera.Views.MainWindow"        x:DataType="app:Composition"        Icon="/Assets/avalonia-logo.ico"        Title="yaEmuera"        DataContext="{StaticResource Composition}">    <Design.DataContext>        <app:Composition />    </Design.DataContext>    <Window.Resources>        <SolidColorBrush x:Key="TextBoxForeground" Color="{Binding MainWindowViewModel.TextBoxForegroundColorProperty}" />        <SolidColorBrush x:Key="TextBoxBackground" Color="{Binding MainWindowViewModel.TextBoxBackgroundColorProperty}" />        <SolidColorBrush x:Key="TextBoxBorderBrushBackground"                         Color="{Binding MainWindowViewModel.TextBoxBorderBackgroundColorProperty}" />    </Window.Resources>    <!-- 也是很神必的Styles了 -->    <Window.Styles>        <Style Selector="TextBox">            <Setter Property="Background" Value="{DynamicResource TextBoxBackground}" />            <Setter Property="Foreground" Value="{DynamicResource TextBoxForeground}" />            <Setter Property="IsReadOnly" Value="True" />            <Setter Property="Focusable" Value="False" />            <Setter Property="CornerRadius" Value="0" />            <Setter Property="BorderBrush" Value="{DynamicResource TextBoxBorderBrushBackground}" />            <Style Selector="^:pointerover">                <Setter Property="Foreground" Value="{DynamicResource TextBoxForeground}" />                <Style Selector="^ /template/ Border#PART_BorderElement">                    <Setter Property="Background" Value="{DynamicResource TextBoxBorderBrushBackground}" />                    <Setter Property="BorderBrush" Value="{DynamicResource TextBoxBorderBrushBackground}" />                </Style>                <Style Selector="^ /template/ DockPanel">                    <Setter Property="Background" Value="{DynamicResource TextBoxBackground}" />                </Style>            </Style>        </Style>        <Style Selector="Menu">            <Setter Property="Padding" Value="0" />        </Style>        <Style Selector="MenuItem">            <Setter Property="Height" Value="{Binding MainWindowViewModel.MenuItemHeight}" />            <Setter Property="HorizontalAlignment" Value="Left" />            <Setter Property="VerticalAlignment" Value="Center" />            <Setter Property="Padding" Value="5, 2, 5, 2" />            <Setter Property="Margin" Value="5, 2, 5, 2" />            <Style Selector="^.secondary-items">                <Setter Property="Width" Value="{Binding MainWindowViewModel.SecondaryMenuItemWidth}" />            </Style>        </Style>        <Style Selector="Separator">            <Setter Property="Background" Value="Black" />        </Style>    </Window.Styles>    <DockPanel>        <Grid DockPanel.Dock="Top" ColumnDefinitions="*,Auto">            <Menu Grid.Column="0" Margin="0,0,5,0">                <MenuItem Header="Files(_F)">                    <MenuItem Classes="secondary-items" Header="Restart(_R)"                              Command="{Binding MainWindowViewModel.RestartCommand}" />                    <MenuItem Classes="secondary-items" Header="Restart with Debug Mode(_D)" />                    <Separator />                    <MenuItem Classes="secondary-items" Header="Save Log(_S)..." InputGesture="Ctrl + S" />                    <MenuItem Classes="secondary-items" Header="Open Clipboard Window(_C)..." InputGesture="Ctrl + C" />                    <Separator />                    <MenuItem Classes="secondary-items" Header="Return to Title Screen(_T)" InputGesture="Ctrl + T" />                    <MenuItem Classes="secondary-items" Header="Reload All Scripts(_C)" />                    <MenuItem Classes="secondary-items" Header="Reload Scripts in(_F)..." />                    <MenuItem Classes="secondary-items" Header="Reload Script(_A)..." />                    <Separator />                    <MenuItem Classes="secondary-items" Header="Exit(_X)" />                </MenuItem>                <MenuItem Header="Help(_H)">                    <MenuItem Classes="secondary-items" Header="Settings..." />                </MenuItem>            </Menu>            <TextBlock Grid.Column="1" HorizontalAlignment="Right" Text="yaEmuera α" VerticalAlignment="Center"                       Margin="0,0,5,0" />        </Grid>        <TextBox>            你好        </TextBox>    </DockPanel></Window>